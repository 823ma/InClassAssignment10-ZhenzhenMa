<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seanma.inclassassignment10_zhenzhenm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_camera"
            android:label="Camera"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".activity_list"
            android:label="Read BlogPosts"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".activity_object"
            android:label="BlogPost Object"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".activity_storage"
            android:label="Load Images"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>